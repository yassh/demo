doctype html
html
  head
    title Demo - Node.appendChild
  body
    script.
      (function() {
        var s = document.createElement('script');
        s.src = 'head.js';
        document.head.appendChild(s);
      })();

      (function() {
        var s = document.createElement('script');
        s.src = 'body.js';
        document.body.appendChild(s);
      })();
